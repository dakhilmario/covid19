{
  "name": "covid-19",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node-sass --output-style=expanded --source-map=true scss/screen.scss css/style.css",
    "prefix": "postcss css/style -u postcss-prefixer -o dist/screen.css",
    "minify": "cleancss --level=1 --source-map --source-map-inline-sources --output css/style.min.css css/style.css",
    "dev": "npm run compile -- --watch",
    "build": "npm run clean && npm run compile && npm run prefix && npm run minify && webpack"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "clean-css-cli": "^4.3.0",
    "node-sass": "^5.0.0",
    "postcss-cli": "^8.3.1",
    "postcss-prefixer": "^2.1.2",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "autoprefixer": "^10.2.4",
    "bootstrap": "^4.6.0",
    "postcss": "^8.2.4",
    "tailwindcss": "^2.0.2",
    "chart.js": "^2.9.4"
  }
}
